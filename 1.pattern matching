#1
import re

def main():
    t= "The quick brown fox jumps over the lazy dog."
    p=r'\b\w{5}\b'
    print("list of matches for the pattern in the text:")
    matches=re.findall(p,t)
    print(matches)
    print("matches found")
    for match in matches:
        print(match)
    
if __name__=="__main__":
    main()
