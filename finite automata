def match_ab(string):
    state = 0
    for char in string:
        if state == 0 and char == 'a':
            state = 1
        elif state == 1 and char == 'b':
            state = 2
        else:
            state = 0
    return state == 2

print(match_ab("cab"))  # False
print(match_ab("ab"))   # True
print(match_ab("acab")) # False
